{
    "name": "idbi-studio/insales-php-api",
    "description": "PHP client for InSales API",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "API",
        "InSales",
        "REST"
    ],
    "homepage": "http://insales.ru/",
    "authors": [
        {
            "name": "Mazepin Vasily",
            "email": "vasily@liven-mv.ru"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "InsalesApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InsalesApi\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
